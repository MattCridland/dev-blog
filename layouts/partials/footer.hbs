    <section class="blog-list">
      <h2>Blog Roll</h2>
      <nav>
      <ul>
      {{#each collections.articles}}
        <li>
          <a href="/{{ this.path }}/">
            <h3>{{ this.title }}</h3>
            <p>{{ this.blurb }}</p>
          </a>
        </li>
      {{/each}}
      </ul>
      </nav>
    </section>

    <footer>
      <p>&copy; Matthew Cridland 2019</p>
    </footer>
  </body>
</html>
